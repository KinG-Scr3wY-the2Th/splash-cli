{
	"ecmaFeatures": {
		"arrowFunctions": true
	},
	"env": {
		"node": true,
		"browser": false,
		"es6": true
	},
	"extends": ["eslint:recommended", "unsplash"],
	"rules": {
		"no-console": "off",
		"require-await": "error",
		"no-nested-ternary": "off"
	},
	"parserOptions": {
		"ecmaVersion": 2018
	}
}